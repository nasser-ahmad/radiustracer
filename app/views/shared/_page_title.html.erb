

  <div class="row">
	<div class="page-header col-lg-12" style="background-color: #ffeecc; direction: rtl; margin-bottom: 30px;padding-bottom: 25px;">
	   <h1 style=" padding: 10px; ">
	   	  <span  class="pull-right"> <%= title %> &nbsp;&nbsp;&nbsp;&nbsp;</span> 
		  <% case title %>
		   <% when 'المزودات' %>
		   	 <%= link_to 'رجوع للصفحة الرئيسية' , root_path , :class => 'btn btn-danger pull-left' %>
				<%= link_to 'إضافة مزود' , new_isp_path , :class => 'btn btn-success pull-right'%>

		   <% when 'العمليات' %>
			  <%= link_to 'رجوع للصفحة الرئيسية' , root_path , :class => 'btn btn-danger pull-left' %> 
		   <% when 'عملية جديدة' %>
			 <%= link_to 'رجوع لصفحة العمليات' , requests_path , :class => 'btn btn-danger pull-left' %>
		   <% when 'تعديل بيانات مزود' %>
		   	 <%= link_to 'رجوع لصفحة المزودات' , isps_path , :class => 'btn btn-danger pull-left' %>
		   <% when /مزود / %>
		   	 <%= link_to 'رجوع لصفحة المزودات' , isps_path , :class => 'btn btn-danger pull-left' %>
		   <% when /الموظف/ %>
		   	 <%= link_to 'رجوع لصفحة المزود' , isp_path(@isp) , :class => 'btn btn-danger pull-left' %>
		   <% when 'عناوين المزودات' %>
		   	 <%= link_to 'رجوع لصفحة المزود' , isp_path(@isp) , :class => 'btn btn-danger pull-left' %>
		   <% when /معلومات/ %>
		   	 <%= link_to 'رجوع لصفحة المزودات' , isps_path , :class => 'btn btn-danger pull-left' %>	   	 
		   <% when /عناوين المزود/ %>
		   	 <%= link_to 'رجوع لصفحة المزود' , isp_path(@isp) , :class => 'btn btn-danger pull-left' %>
		   	<% when 'موظف جديد' %>
		   	 <%= link_to 'رجوع لصفحة المزود' , isp_path(@isp) , :class => 'btn btn-danger pull-left' %> 	   	 
		   	<% when /ملاحظة مزود/ %>
		   	 <%= link_to 'رجوع لصفحة المزود' , isp_path(@isp) , :class => 'btn btn-danger pull-left' %> 
		   	<% when /مجال/ %>
		   	 <%= link_to 'رجوع ' , isp_path(@ip_range.isp_id) , :class => 'btn btn-danger pull-left' %> 
		   	<% when 'تعديل العناوين' %>
		   	 <%= link_to 'رجوع ' , isp_path(@ip_range.isp_id) , :class => 'btn btn-danger pull-left' %> 
		   	<% when 'حساب جديد' %>
		   	 <%= link_to 'رجوع للصفحة الرئيسية ' , root_path , :class => 'btn btn-danger pull-left' %>   
		  <% end %>
	   </h1>
	  </div>


  </div>